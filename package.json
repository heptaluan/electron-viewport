{
  "name": "electron-vue-vite",
  "version": "1.0.0",
  "main": "dist/main/index.cjs",
  "author": "草鞋没号 <308487730@qq.com>",
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/watch.mjs",
    "prebuild": "vue-tsc --noEmit --p packages/renderer/tsconfig.json && node scripts/build.mjs",
    "build": "electron-builder",
    "init": "git config core.hooksPath .git/hooks/ && rm -rf .git/hooks && npx simple-git-hooks",
    "test:e2e": "npx playwright test",
    "test:e2e:headless": "npx playwright test --headed"
  },
  "engines": {
    "node": ">=14.17.0"
  },
  "dependencies": {
    "sqlite3": "^5.0.2"
  },
  "devDependencies": {
    "@playwright/test": "^1.20.2",
    "@types/sqlite3": "^3.1.8",
    "@vitejs/plugin-vue": "^2.1.0",
    "ant-design-vue": "^2.2.8",
    "dicom-parser": "^1.8.13",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "nano-staged": "^0.6.0",
    "sass": "^1.50.1",
    "simple-git-hooks": "^2.7.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.1",
    "vite-plugin-electron": "^0.4.2",
    "vite-plugin-resolve": "^2.0.1",
    "vue": "^3.2.31",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.31.1",
    "cornerstone-core": "^2.6.0",
    "cornerstone-math": "^0.1.9",
    "cornerstone-tools": "^6.0.1",
    "cornerstone-wado-image-loader": "^4.0.4"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "rollup",
    "vite",
    "vue3",
    "vue"
  ]
}
